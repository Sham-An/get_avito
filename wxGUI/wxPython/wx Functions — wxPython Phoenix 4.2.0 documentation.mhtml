<span></span><span class=3D"n">answer</span> <span class=3D"o">=3D</span> <=
span class=3D"n">wx</span><span class=3D"o">.</span><span class=3D"n">Messa=
geBox</span><span class=3D"p">(</span><span class=3D"s2">"Quit program?"</s=
pan><span class=3D"p">,</span> <span class=3D"s2">"Confirm"</span><span cla=
ss=3D"p">,</span>
                       <span class=3D"n">wx</span><span class=3D"o">.</span=
><span class=3D"n">YES_NO</span> <span class=3D"o">|</span> <span class=3D"=
n">wx</span><span class=3D"o">.</span><span class=3D"n">CANCEL</span><span =
class=3D"p">,</span> <span class=3D"n">main_frame</span><span class=3D"p">)=
</span>
<span class=3D"k">if</span> <span class=3D"n">answer</span> <span class=3D"=
o">=3D=3D</span> <span class=3D"n">wx</span><span class=3D"o">.</span><span=
 class=3D"n">YES</span><span class=3D"p">:</span>
    <span class=3D"n">main_frame</span><span class=3D"o">.</span><span clas=
s=3D"n">Close</span><span class=3D"p">()</span>
